pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'echo Construyendo el codigo ...'
      }
    }

    stage('Test') {
      steps {
        sh 'echo Probando el codigo'
      }
    }

    stage('Deploy') {
      steps {
        sh 'Entregando el codigo'
      }
    }

  }
}